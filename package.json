{
  "name": "rentspace",
  "version": "1.0.0",
  "description": "Social network for renting cars",
  "main": "server.js",
  "scripts": {
    "start": "set NODE_ENV=production && node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "client": "file:client",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.8.1",
    "gravatar": "^1.8.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongo-image-converter": "^1.0.2",
    "mongoose": "^5.11.8",
    "multer": "^1.4.2",
    "normalize-url": "^5.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  },
  "engines": {
    "node": "14.6.0"
  }
}
